{
  "name": "pokemail",
  "version": "0.5.0",
  "description": "Check whether an email exists and if there are problems with it",
  "main": "lib/pokemail.js",
  "scripts": {
    "compile": "babel --presents env --ignore *.test.js -d lib src",
    "start": "node .",
    "test": "tape -r babel-register 'src/**/*.test.js' | faucet",
    "lint": "eslint src",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunohodov/pokemail.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lunohodov/pokemail/issues"
  },
  "homepage": "https://github.com/lunohodov/pokemail#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "faucet": "^0.0.1",
    "husky": "^0.14.3",
    "tape": "^4.8.0"
  }
}
